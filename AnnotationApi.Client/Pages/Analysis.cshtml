@page
@model AnalysisModel
@{
    ViewData["Title"] = "Analyses";
}
<h1>@ViewData["Title"]</h1>

@Html.AntiForgeryToken()

<div class="text-center">
    <div class="row">
        <div class="annotations col-md-4">
            @foreach (var a in Model.Analyses)
            {
                <p>
                    <a href="/index?id=@a.DbId">@a.Caption</a>
                </p>
            }
        </div>
    </div>
</div>